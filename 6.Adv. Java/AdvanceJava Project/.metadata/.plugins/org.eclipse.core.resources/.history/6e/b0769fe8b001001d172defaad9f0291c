import com.cdac.dao.UserDao;

public class App {
	public static void main(String[] args) {
		//Loading Spring/IOC Container
		ApplicationContext ctx = new ClassPathXmlApplicationContext("my-spring-config.xml");
		
		UserDao dao=new UserDao();
	}
	
}
